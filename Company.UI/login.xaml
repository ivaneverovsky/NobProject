<Window x:Class="Company.UI.login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Company.UI"
        mc:Ignorable="d"
        Title="login" Height="350" Width="525">
    <Grid Margin="0,0,2,0">
        <TextBox x:Name="loginBox" HorizontalAlignment="Left" Height="23" Margin="207,161,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120" TextChanged="loginBox_TextChanged"/>
        <TextBox x:Name="passwordBox" HorizontalAlignment="Left" Height="23" Margin="207,189,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
        <Button x:Name="enter" Content="Войти" HorizontalAlignment="Left" Margin="207,217,0,0" VerticalAlignment="Top" Width="58" Click="enter_Click"/>
        <Button x:Name="reg" Content="Рег" HorizontalAlignment="Left" Margin="270,217,0,0" VerticalAlignment="Top" Width="57" Click="reg_Click"/>
        <Button x:Name="ConnectToServer" Content="Connect" HorizontalAlignment="Left" Margin="10,289,0,0" VerticalAlignment="Top" Width="75" Click="ConnectToServer_Click"/>

    </Grid>
</Window>
