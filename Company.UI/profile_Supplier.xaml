<Window x:Class="Company.UI.profile_Supplier"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Company.UI"
        mc:Ignorable="d"
        Title="profile_Supplier">
    <Grid RenderTransformOrigin="0.474,0.555" Margin="0,0,-155.667,-62.667">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition Width="0*"/>
            <ColumnDefinition Width="17*"/>
            <ColumnDefinition Width="900*"/>
        </Grid.ColumnDefinitions>
        <ListBox x:Name="listBox_CatalogueOfOrders" HorizontalAlignment="Left" Height="117" Margin="9.667,199,0,0" VerticalAlignment="Top" Width="185" MouseDoubleClick="listBox_CatalogueOfOrders_MouseDoubleClick" Grid.Column="5"/>
        <Label x:Name="labelCatalogue" Content="   Catalogue" HorizontalAlignment="Left" Margin="59.667,151,0,0" VerticalAlignment="Top" Grid.Column="5" Height="43" Width="94"/>
        <Label x:Name="label_Orders" Content="Orders" HorizontalAlignment="Left" Margin="570.667,151,0,0" VerticalAlignment="Top" Grid.Column="5" Width="76"/>
