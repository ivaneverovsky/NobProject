        <Button x:Name="button_Show" Content="Show catalogue" HorizontalAlignment="Left" Margin="59.667,346,0,0" VerticalAlignment="Top" Width="94" Click="button_Show_Click" Grid.Column="5"/>
        <Label x:Name="label" Content="Name" Grid.Column="5" HorizontalAlignment="Left" Margin="59.667,34,0,0" VerticalAlignment="Top"/>
        <Label x:Name="label_Copy" Content="Surname&#xD;&#xA;" Grid.Column="5" HorizontalAlignment="Left" Margin="262.667,34,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="SupplierNameBox" Grid.Column="5" HorizontalAlignment="Left" Height="23" Margin="106.667,36,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <TextBox x:Name="SupplierSurnameBox" Grid.Column="5" HorizontalAlignment="Left" Height="23" Margin="324.667,37,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
    </Grid>
</Window>
